<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" minWidth="833.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.view.TrainingController">
   <children>
      <Button fx:id="lancement" layoutX="208.0" layoutY="77.0" mnemonicParsing="false" onMouseClicked="#lancementApplication" text="Start">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="pause" layoutX="393.0" layoutY="77.0" mnemonicParsing="false" text="Pause">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="stop" layoutX="579.0" layoutY="77.0" mnemonicParsing="false" text="Stop">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ProgressIndicator fx:id="progressCircle" layoutX="405.0" layoutY="355.0" prefHeight="87.0" prefWidth="102.0" progress="0.0" />
      <ProgressBar fx:id="progressBar" layoutX="232.0" layoutY="268.0" prefHeight="25.0" prefWidth="386.0" progress="0.0" />
      <TextField fx:id="errorAffichage" layoutX="259.0" layoutY="173.0" prefHeight="31.0" prefWidth="332.0" />
      <Button fx:id="retourMenu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#retourMenu" text="Retour">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
